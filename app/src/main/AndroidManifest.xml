    package="com.wa.dev.edukasikomputer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EdukasiKomputer">
        <activity android:name=".PerangkatProses"></activity>
        <activity android:name=".PerangkatKeluaran" />
        <activity android:name=".PerangkatMasukan" />
        <activity android:name=".HardwareKomputer" />
        <activity android:name=".About" />
        <activity android:name=".DefinisiKomputer" />
        <activity android:name=".MenuKe2" />
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>